{
  "name": "hardhat-foundry",
  "version": "0.0.1",
  "description": "Forkable Hardhat & Foundry template",
  "scripts": {
    "build": "hardhat compile",
    "test": "hardhat test",
    "test:forge": "forge test -vvv",
    "doc": "forge doc",
    "type": "hardhat typechain",
    "lint:check": "prettier --check '**.{sol,js,ts}'",
    "lint:fix": "prettier --write '**.{sol,js,ts}'",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/massun-onibakuchi/hardhat-foundry-template/"
  },
  "keywords": [
    "hardhat",
    "solidity",
    "foundry"
  ],
  "author": "0xbakuchi",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/massun-onibakuchi/hardhat-foundry-template/#readme",
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.5",
    "@nomicfoundation/hardhat-foundry": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.7",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-etherscan": "^3.1.5",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.24",
    "chai": "^4.3.7",
    "dotenv": "^16.0.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.9.3",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "lint-staged": {
    "*.sol": "prettier --write",
    "*.js": "prettier --write",
    "*.ts": "prettier --write"
  },
  "dependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "typechain": "^8.1.1"
  },
  "private": "false",
  "resolutions": {
    "async": ">=2.6.4",
    "cross-fetch": ">=3.1.5",
    "lodash": ">=4.17.21",
    "node-fetch": ">=2.6.7",
    "underscore": ">=1.12.1",
    "yargs-parser": ">=5.0.1"
  }
}
